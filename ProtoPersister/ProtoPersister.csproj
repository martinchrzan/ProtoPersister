<Project Sdk="Microsoft.NET.Sdk" ToolsVersion="15.0">
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>
    <Version>0.1.0</Version>
    <Authors>Martin Chrzan</Authors>
    <Company />
    <Description>Persistence framework build on top of protobuf-net</Description>
    <Copyright>Martin Chrzan (c) 2018</Copyright>
    <PackageProjectUrl>https://github.com/martinchrzan/ProtoPersister/</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/martinchrzan/ProtoPersister/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/martinchrzan/ProtoPersister/</RepositoryUrl>
    <RepositoryType>github</RepositoryType>
    <PackageTags>protobuf, persistence</PackageTags>
    <PackageReleaseNotes>First version</PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="protobuf-net" Version="2.3.7" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>
</Project>